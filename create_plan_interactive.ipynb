  {
   "cell_type": "code",
   "id": "pdt7l9hzown",
   "source": "from IPython.display import display, HTML\nimport json\n\n# Cores para cada tipo de treino\nWORKOUT_COLORS = {\n    'easy': '#90EE90',        # Verde claro\n    'recovery': '#98FB98',    # Verde p√°lido\n    'long': '#87CEEB',        # Azul claro\n    'long_run': '#87CEEB',    # Azul claro\n    'tempo': '#FFD700',       # Dourado\n    'threshold': '#FFA500',   # Laranja\n    'interval': '#FF6B6B',    # Vermelho claro\n    'intervals': '#FF6B6B',   # Vermelho claro\n    'repetition': '#FF69B4',  # Rosa\n    'rest': '#D3D3D3',        # Cinza claro\n    'cross': '#DDA0DD',       # Ameixa\n    'cross_training': '#DDA0DD',\n    'race': '#9370DB',        # Roxo m√©dio\n    'test_race': '#BA55D3',   # Orqu√≠dea m√©dio\n}\n\ndef get_workout_color(workout_type: str) -> str:\n    \"\"\"Retorna a cor correspondente ao tipo de treino.\"\"\"\n    workout_lower = workout_type.lower().replace(' ', '_').replace('-', '_')\n    for key, color in WORKOUT_COLORS.items():\n        if key in workout_lower or workout_lower in key:\n            return color\n    return '#F5F5F5'  # Cor padr√£o\n\ndef format_duration(minutes: float) -> str:\n    \"\"\"Formata dura√ß√£o em minutos para formato leg√≠vel.\"\"\"\n    if not minutes:\n        return '-'\n    hours = int(minutes // 60)\n    mins = int(minutes % 60)\n    if hours > 0:\n        return f'{hours}h{mins:02d}min'\n    return f'{mins}min'\n\ndef display_training_plan(plan):\n    \"\"\"Exibe o plano de treinamento em uma tabela HTML colorida.\"\"\"\n    if plan is None:\n        print('Nenhum plano gerado ainda. Clique em \"Salvar JSON e gerar plano\" primeiro.')\n        return\n    \n    plan_dict = plan.to_dict() if hasattr(plan, 'to_dict') else plan\n    \n    # Cabe√ßalho do plano\n    html = f'''\n    <style>\n        .plan-container {{\n            font-family: Arial, sans-serif;\n            max-width: 100%;\n            overflow-x: auto;\n        }}\n        .plan-header {{\n            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n            color: white;\n            padding: 20px;\n            border-radius: 10px 10px 0 0;\n            margin-bottom: 0;\n        }}\n        .plan-header h2 {{\n            margin: 0 0 10px 0;\n        }}\n        .plan-info {{\n            display: flex;\n            gap: 20px;\n            flex-wrap: wrap;\n        }}\n        .plan-info span {{\n            background: rgba(255,255,255,0.2);\n            padding: 5px 12px;\n            border-radius: 15px;\n            font-size: 14px;\n        }}\n        .week-container {{\n            margin-bottom: 20px;\n            border: 1px solid #ddd;\n            border-radius: 8px;\n            overflow: hidden;\n        }}\n        .week-header {{\n            background: #4a5568;\n            color: white;\n            padding: 12px 15px;\n            font-weight: bold;\n            display: flex;\n            justify-content: space-between;\n            align-items: center;\n        }}\n        .week-phase {{\n            background: rgba(255,255,255,0.2);\n            padding: 3px 10px;\n            border-radius: 10px;\n            font-size: 12px;\n        }}\n        .week-table {{\n            width: 100%;\n            border-collapse: collapse;\n        }}\n        .week-table th {{\n            background: #e2e8f0;\n            padding: 10px;\n            text-align: left;\n            font-size: 13px;\n            border-bottom: 2px solid #cbd5e0;\n        }}\n        .week-table td {{\n            padding: 12px 10px;\n            border-bottom: 1px solid #e2e8f0;\n            vertical-align: top;\n        }}\n        .workout-type {{\n            display: inline-block;\n            padding: 4px 10px;\n            border-radius: 12px;\n            font-size: 12px;\n            font-weight: bold;\n            text-transform: uppercase;\n        }}\n        .workout-details {{\n            font-size: 13px;\n            color: #4a5568;\n            margin-top: 5px;\n        }}\n        .workout-duration {{\n            font-size: 12px;\n            color: #718096;\n        }}\n        .legend {{\n            display: flex;\n            flex-wrap: wrap;\n            gap: 10px;\n            padding: 15px;\n            background: #f7fafc;\n            border-radius: 0 0 10px 10px;\n            margin-bottom: 20px;\n        }}\n        .legend-item {{\n            display: flex;\n            align-items: center;\n            gap: 5px;\n            font-size: 12px;\n        }}\n        .legend-color {{\n            width: 20px;\n            height: 20px;\n            border-radius: 4px;\n        }}\n        .summary-box {{\n            background: #f7fafc;\n            border: 1px solid #e2e8f0;\n            border-radius: 8px;\n            padding: 15px;\n            margin-bottom: 20px;\n        }}\n        .summary-grid {{\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));\n            gap: 15px;\n        }}\n        .summary-item {{\n            text-align: center;\n        }}\n        .summary-value {{\n            font-size: 24px;\n            font-weight: bold;\n            color: #4a5568;\n        }}\n        .summary-label {{\n            font-size: 12px;\n            color: #718096;\n        }}\n    </style>\n    <div class=\"plan-container\">\n        <div class=\"plan-header\">\n            <h2>{plan_dict.get('name', 'Plano de Treinamento')}</h2>\n            <div class=\"plan-info\">\n                <span>Objetivo: {plan_dict.get('goal', '-')}</span>\n                <span>N√≠vel: {plan_dict.get('level', '-')}</span>\n                <span>{plan_dict.get('days_per_week', '-')} dias/semana</span>\n                <span>{len(plan_dict.get('weeks', []))} semanas</span>\n            </div>\n        </div>\n        <div class=\"legend\">\n            <strong>Legenda:</strong>\n    '''\n    \n    # Adicionar legenda\n    for workout_type, color in WORKOUT_COLORS.items():\n        if workout_type not in ['long_run', 'intervals', 'cross_training']:  # Evitar duplicatas\n            html += f'<div class=\"legend-item\"><div class=\"legend-color\" style=\"background:{color}\"></div>{workout_type.replace(\"_\", \" \").title()}</div>'\n    \n    html += '</div>'\n    \n    # Calcular resumo\n    total_km = 0\n    total_sessions = 0\n    weeks_data = plan_dict.get('weeks', [])\n    \n    for week in weeks_data:\n        for session in week.get('sessions', []):\n            total_sessions += 1\n            total_km += session.get('distance_km', 0) or 0\n    \n    html += f'''\n    <div class=\"summary-box\">\n        <div class=\"summary-grid\">\n            <div class=\"summary-item\">\n                <div class=\"summary-value\">{len(weeks_data)}</div>\n                <div class=\"summary-label\">Semanas</div>\n            </div>\n            <div class=\"summary-item\">\n                <div class=\"summary-value\">{total_sessions}</div>\n                <div class=\"summary-label\">Sess√µes totais</div>\n            </div>\n            <div class=\"summary-item\">\n                <div class=\"summary-value\">{total_km:.1f} km</div>\n                <div class=\"summary-label\">Dist√¢ncia total</div>\n            </div>\n            <div class=\"summary-item\">\n                <div class=\"summary-value\">{total_km/len(weeks_data):.1f} km</div>\n                <div class=\"summary-label\">M√©dia semanal</div>\n            </div>\n        </div>\n    </div>\n    '''\n    \n    # Tabelas por semana\n    for week in weeks_data:\n        week_num = week.get('week_number', '?')\n        phase = week.get('phase', '')\n        week_km = sum(s.get('distance_km', 0) or 0 for s in week.get('sessions', []))\n        \n        html += f'''\n        <div class=\"week-container\">\n            <div class=\"week-header\">\n                <span>Semana {week_num} - {week_km:.1f} km</span>\n                <span class=\"week-phase\">{phase}</span>\n            </div>\n            <table class=\"week-table\">\n                <tr>\n                    <th style=\"width:100px\">Dia</th>\n                    <th style=\"width:120px\">Tipo</th>\n                    <th>Descri√ß√£o</th>\n                    <th style=\"width:80px\">Dist√¢ncia</th>\n                    <th style=\"width:80px\">Dura√ß√£o</th>\n                </tr>\n        '''\n        \n        for session in week.get('sessions', []):\n            workout_type = session.get('type', session.get('workout_type', 'easy'))\n            color = get_workout_color(workout_type)\n            day = session.get('day', session.get('day_of_week', '-'))\n            description = session.get('description', session.get('details', '-'))\n            distance = session.get('distance_km', 0)\n            duration = session.get('duration_minutes', session.get('duration', 0))\n            \n            distance_str = f'{distance:.1f} km' if distance else '-'\n            duration_str = format_duration(duration)\n            \n            html += f'''\n                <tr>\n                    <td><strong>{day}</strong></td>\n                    <td><span class=\"workout-type\" style=\"background:{color}\">{workout_type}</span></td>\n                    <td class=\"workout-details\">{description}</td>\n                    <td class=\"workout-duration\">{distance_str}</td>\n                    <td class=\"workout-duration\">{duration_str}</td>\n                </tr>\n            '''\n        \n        html += '</table></div>'\n    \n    html += '</div>'\n    \n    display(HTML(html))\n\n# Executar visualiza√ß√£o se o plano j√° foi gerado\nif 'generated_plan' in dir() and generated_plan is not None:\n    display_training_plan(generated_plan)\nelse:\n    print('Preencha o formul√°rio acima e clique em \"Salvar JSON e gerar plano\" para visualizar o plano aqui.')",
